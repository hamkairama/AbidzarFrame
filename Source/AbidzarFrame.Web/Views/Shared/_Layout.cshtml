    <script>
        jQuery(document).ready(function () {
            Metronic.init(); // init metronic core components
            Layout.init(); // init current layout
            QuickSidebar.init(); // init quick sidebar
            UITree.init();// init tree
        });

        $(function () {
            //parsing the unobtrusive attributes when we get content via ajax
            $(document).ajaxComplete(function () {
                $.validator.unobtrusive.parse(document);
            });

            $.ajaxSetup({
                cache: false
            });

            //$('.modal').on('hidden.bs.modal', function () {
            //    $(this).find('input').val('');
            //});

            $.validator.unobtrusive.options = {
                errorPlacement: function (error, element) {
                    $(element).closest(".form-group").addClass("has-error");
                    //$(element).closest(".field-validation-error");
                },
                success: function (label, element) {
                    $(element).closest(".form-group").removeClass("has-error");
                },
                highlight: function (element) {
                    $(element).closest(".form-group").addClass("has-error");
                },
                unhighlight: function (element) {
                    $(element).closest(".form-group").removeClass("has-error");
                }
            }
        });

        $(function () {
            function setModalMaxHeight(element) {
                this.$element = $(element);
                this.$content = this.$element.find('.modal-content');
                var borderWidth = this.$content.outerHeight() - this.$content.innerHeight();
                var dialogMargin = $(window).width() > 767 ? 60 : 20;
                var contentHeight = $(window).height() - (dialogMargin + borderWidth) + 35;
                var headerHeight = this.$element.find('.modal-header').outerHeight() || 0;
                var footerHeight = this.$element.find('.modal-footer').outerHeight() || 0;
                var maxHeight = contentHeight - (headerHeight + footerHeight);

                this.$content.css({
                    'overflow': 'hidden'
                });

                this.$element
                  .find('.modal-body').css({
                      'max-height': maxHeight,
                      'overflow-y': 'auto'
                  });
            }

            //$('.modal-dialog').draggable();

            $('.modal').on('show.bs.modal', function () {
                $(this).show();
                setModalMaxHeight(this);
            });

            $(window).resize(function () {
                if ($('.modal.in').length != 0) {
                    setModalMaxHeight($('.modal.in'));
                }
            });

            $('.modal').on('shown', function () {
                var offset = $(this).offset().top;
                $(window).scrollTop(offset);
            });

            $('body').on('hidden.bs.modal', '.modal', function () {
                $(this).removeData('bs.modal');
                $(this).find('input').val('');
            });
        });
        //$(document).ready(function () {


        //    $("select").prop('class', 'selectpicker show-tick form-control');
        //    $("select").attr('data-live-search', true);
        //    $("select").attr('size', 5);

        //});

        $('#searchQuickMenu').autocomplete({
            minLength: 2,
            source: function (request, response) {
                $.ajax({
                    url: '@Url.Action("GetQuickSearchMenus", "ManageLayout", new { area = ""})',
                    dataType: "json",
                    data: { search: $('#searchQuickMenu').val() },
                    success: function (data) {
                        response($.map(data, function (item) {
                            return { label: item.Text, value: item.Value };
                        }));
                    },
                    error: function (xhr, status, error) {
                        alertError('error');
                    }
                });
            },
            select: function (event, ui) {
                //console.log(ui.item.value);
                showLoading();
                window.location.href = ui.item.value;
            }
        });


